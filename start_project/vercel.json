{
    "builds": [
      {
        "src": "start_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
        "maxLambdaSize": "500mb",
        "runtime": "python3.9"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "start_project/wsgi.py"
      }
    ]
  }